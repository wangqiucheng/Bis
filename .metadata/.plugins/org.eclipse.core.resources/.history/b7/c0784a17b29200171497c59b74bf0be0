package com.bisa.hkshop.model;

import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;

@Entity
@Table(name="s_address",index={@Index(columnList="username")},uniqueConstraints={@UniqueConstraint(columnNames={"addr_number"})})
public class Address {
	private	int	Id;		//Id(鑷id)
	private	String addr_number;		//	64	鍦板潃缂栧彿
	private	String username;		//	36	鐢ㄦ埛鍚�
	private	String address;		//	100	璇︾粏鍦板潃
	private	String tel;		//	20	鐢佃瘽
	private	String name;		//	20	鏀惰揣浜�
	private	String postcode;		//	10	閭斂缂栫爜
	private	String act_time;		//	50	鏈�杩戜娇鐢ㄦ椂闂�
	private	String province;		//	30	鐪�
	private	String city;		//	30	甯�
	private	String county;		//	30	鍘�
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	@Column(length=64,unique=true)
	public String getAddr_number() {
		return addr_number;
	}
	public void setAddr_number(String addr_number) {
		this.addr_number = addr_number;
	}
	@Column(length=36,nullable=false)
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	@Column(length=100,nullable=false)
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	@Column(length=20,nullable=false)
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	@Column(length=20,nullable=false)
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Column(length=10)
	public String getPostcode() {
		return postcode;
	}
	public void setPostcode(String postcode) {
		this.postcode = postcode;
	}
	@Column(length=50)
	public String getAct_time() {
		return act_time;
	}
	public void setAct_time(String act_time) {
		this.act_time = act_time;
	}
	@Column(length=30,nullable=false)
	public String getProvince() {
		return province;
	}
	public void setProvince(String province) {
		this.province = province;
	}
	@Column(length=30,nullable=false)
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	@Column(length=30,nullable=false)
	public String getCounty() {
		return county;
	}
	public void setCounty(String county) {
		this.county = county;
	}
	public Address() {
		super();
	}
	public Address(int id, String addr_number, String username, String address, String tel, String name,
			String postcode, String act_time, String province, String city, String county) {
		super();
		Id = id;
		this.addr_number = addr_number;
		this.username = username;
		this.address = address;
		this.tel = tel;
		this.name = name;
		this.postcode = postcode;
		this.act_time = act_time;
		this.province = province;
		this.city = city;
		this.county = county;
	}

	
	
}
