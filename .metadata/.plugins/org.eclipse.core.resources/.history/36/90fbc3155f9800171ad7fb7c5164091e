var path = "";

$(document).ready(function() {
	path=$("base").attr("href");
});

	function del_shop(){
		var deleteId=$("input[name='deleteId']").val();	
		var userId=$("input[name='userId']").val();
		$.ajax({
			url : path + "delCart",
			type : "post",
			dataType : "text",
			async : false,
			data : {
				"deleteId" : deleteId,
				
			},
			success : function(result) {
				if(result=="success"){
					window.location.href=path+"Cart?&userId="+userId;
				}	
				
			},error:function(){
				
			}

		});
	}
	
	function addshop(){

		var serveOne=$("input[name='serveOne']").val();
		var servetwo=$("input[name='servetwo']").val();
		var servethree=$("input[name='servethree']").val();
		if((serveOne!=""||serveOne!=null) && (servetwo==""||servetwo==null) && (servethree==""||servethree==null)){
			service_number=serveOne;
		}else if((serveOne==""||serveOne==null) && (servetwo!=""||servetwo!=null) && (servethree==""||servethree==null)){
			service_number=servetwo;
		}else{
			service_number=servethree;
		}

