function add_shop(){
	$("add_product").attr("disable",true);
	if (this.ajaxRequest_ != undefined && this.ajaxRequest_.readyState < 4) {
		layer.alert("请稍后", {
			yes : function(ly) {
				layer.close(ly);
			}
		})
		return false;
	}
	var shop_number=$("input[name='shop_number']").val();
	var userId=$("input[name='userId']").val();
	this.ajaxRequest_ = $.ajax({
		url : path + "Cart",
		type : 'POST',
		cache : false,
		async : false,
		data : ,
