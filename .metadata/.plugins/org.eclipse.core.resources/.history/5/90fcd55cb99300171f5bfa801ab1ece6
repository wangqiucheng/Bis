@Repository
public class CartDaoImpl extends BaseDao<Cart> implements IcartDao{

	@Override
	public Cart addCart(Cart cart) {
		// TODO Auto-generated method stub
		return super.add(cart);
	}

	@Override
	public void updateCart(Cart cart) {
		// TODO Auto-generated method stub
		super.update(cart);
	}

	@Override
	public int selCart(String userId, String shop_number) {
		// TODO Auto-generated method stub
		String sql="select * from s_cart AS c where c.username=? and c.shop_number=?";
	}
	

}
