package com.bisa.hkshop.wqc.dao;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.bisa.hkshop.model.Package;
import com.bisa.hkshop.wqc.basic.dao.BaseDao;
@Repository
public class PackageDaoImpl extends BaseDao<Package> implements IPackageDao {


	@Override
	public List<Package> getActiveByNumber(String pmainId) {
		// TODO Auto-generated method stub
		String sql="select * from s_package AS p where p.pmainId=?";
		return super.queryListBySql(sql, new Object[]{pmainId}, Package.class);
	}

}
