package com.bisa.hkshop.wqc.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.bisa.hkshop.model.Active;
import com.bisa.hkshop.model.Classification;
import com.bisa.hkshop.wqc.dao.IActiveDao;
import com.bisa.hkshop.wqc.dao.IClassificationDao;
@Service
public class ClassificationServiceImpl implements IActiveService {

	
	@Autowired
	private IClassificationDao iClassificationDao;
	@Override
	public Classification getClassification() {
		return iClassificationDao.getClassification();
	}
	

}
