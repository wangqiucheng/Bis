package com.bisa.hkshop.wqc.dao;

import org.springframework.stereotype.Repository;

import com.bisa.hkshop.model.Active;
import com.bisa.hkshop.model.Classification;
import com.bisa.hkshop.wqc.basic.dao.BaseDao;
@Repository
public class BannerDaoImpl extends BaseDao<Classification> implements IClassificationDao {

	@Override
	public Classification getClassification() {
		String sql = "select * from s_classification";
		return super.queryObjectBySql(sql, new Object[]{}, Classification.class);
	}

}
