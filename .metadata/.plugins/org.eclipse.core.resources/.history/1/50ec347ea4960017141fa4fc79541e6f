var path = "";

$(document).ready(function() {
	path=$("base").attr("href");
});
function add_shop(){
	$("add_product").attr("disable",true);
	if (this.ajaxRequest_ != undefined && this.ajaxRequest_.readyState < 4) {
		layer.alert("请稍后", {
			yes : function(ly) {
				layer.close(ly);
			}
		})
		return false;
	}
	var package_number=$("input[name='package_number']").val();
	var userId=$("input[name='userId']").val();
	var comm_number=$("input[name='comm_number']").val();
	$.ajax({
		url : path + "Cart",
		type : "post",
		dataType : "json",
		async : false,
		data : {
			"package_number" : package_number,
			"userId":userId,
			"comm_number":comm_number
		},
		success : function(list) {
			console.log(list);
			
			
		}

	});
}