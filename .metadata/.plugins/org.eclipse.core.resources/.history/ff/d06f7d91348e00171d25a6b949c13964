package com.test.u;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import com.bisa.hkshop.model.Banner;
import com.bisa.hkshop.wqc.service.IBannerService;

public class TTTTTTEST {
	
	
	@Autowired
	private IBannerService IBannerService;
	@Cacheable(value=spring_cache,key="#report_status")
	public  List<Banner> getIb() {
		List<Banner> banner=IBannerService.getBanner();
		return banner;
	}
}
